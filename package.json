		"@types/jest": "^29.4.0",
		"@types/node": "^17.0.10",
		"jest": "^29.0.0",
		"prettier": "^2.5.1",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"files": [
		"lib/**/*",
