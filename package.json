{
	"name": "vect-ops",
	"version": "0.0.1",
	"description": "High performance vector operations in JavaScipt",
	"main": "./lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"test": "npx jest",
		"build": "tsc",
		"lint": "npx eslint . --ext .ts",
		"lint-fix": "npx eslint . --ext .ts --fix",
		"format": "npx prettier --write \"src/**/*.ts\"",
		"prepare": "npm run format && npm run lint-fix && npm run build && npm test",
		"preversion": "npm run lint",
		"prepublish": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jiricekcz/vect-ops.git"
	},
	"author": "jiricekcz",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jiricekcz/vect-ops/issues"
	},
	"homepage": "https://github.com/jiricekcz/vect-ops#readme",
	"devDependencies": {
		"@types/node": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.25.4",
		"jest": "^27.4.7",
		"prettier": "^2.5.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"files": [
		"lib/**/*",
		"!lib/**/*.test.js",
		"./README.md",
		"./LICENSE",
		"./package.json",
		"./package-lock.json",
		"./tsconfig.json",
		"./*"
	]
}
